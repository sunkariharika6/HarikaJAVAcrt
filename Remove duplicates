import java.util.*;
class Test
{
public static void main(String[] args)
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int [n];
int i;
for(i=0;i<n;i++)
{
a[i]=s.nextInt();
}
int j=0;
for(i=0;i<n-1;i++)
{
 if(a[i]!=a[i+1])
{
a[j++]=a[i];
}
}
a[j]=a[n-1];
for(i=0;i<j+1;i++)
{
System.out.print(a[i]);
}
}
}
output:C:\javacrt>java Test
6
1 2 2 3 3 4
1234
